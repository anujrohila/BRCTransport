@model BRCTransport.Domain.tblConsignmentNoteDTO
@using Telerik.Web.Mvc.UI
@{
    
}
<div id="hiddenValues">
    @Html.Hidden("ActiveMenuName", "LRNote")
</div>
<link href="~/Content/Styles/CommonStyle.css" rel="stylesheet" />
<link href="~/Content/Styles/ConsignmentNote.css" rel="stylesheet" />
@using (Html.BeginForm("Save", "ConsignmentNote", FormMethod.Post, new { id = "frmConsignmentNoteInfo", @class = "form-horizontal" }))
{
    <div id="windows">
        <div id="modelConsignmentNoteEntryPopup" class="modal  fade" tabindex="-1" data-focus-on="input:first" data-width="600">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true" onclick="OnCloseConsignmentNoteEntry();">×</button>
                <h3>Save ConsignmentNote Order</h3>
            </div>
            <div id="modelConsignmentNoteEntryPopupContent" class="modal-body" style="max-height: 430px;">
            </div>
        </div>
    </div>
    <div id="hiddenFieldsSave">
        @Html.HiddenFor(model => model.ConsignmentId)
    </div>
    <section class="title">
    </section>
    <section id="about-us" class="container ContentBackground">
        <div class="row-fluid">
            <div class="span12">
                <label class="PageTitleHeading">Save ConsignmentNote</label>
            </div>
            <div class="span12">
                @Html.ValidationSummary()
            </div>
        </div>
        <div class="row-fluid">
            <table>
                <tr>
                    <td colspan="3">
                        <table style="width: 1100px">
                            <tr>
                                <td style="width: 530px">
                                    <table>
                                        <tr>
                                            <td style="width: 115px">Company</td>
                                            <td style="width: 140px">
                                                @Html.TextBoxFor(model => model.CompanyName, new { @class = "width115" })
                                            </td>
                                            <td style="width: 120px">Policy No.</td>
                                            <td style="width: 140px">
                                                @Html.TextBoxFor(model => model.PolicyNo, new { @class = "width115" })
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Date</td>
                                            <td>
                                                @(Html.Telerik().DatePickerFor(m => m.CompanyDate).InputHtmlAttributes(new { @class = "input-block-level" }))
                                            </td>
                                            <td>Amount</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.CompanyAmount, new { @class = "width115" })
                                            </td>
                                        </tr>
                                        <tr>
                                            <td colspan="4">Consignee&#39;s Name &amp; Address with Phone NO.</td>
                                        </tr>
                                        <tr>
                                            <td colspan="4">
                                                @Html.DropDownListFor(m => m.ConsigneeId, new SelectList(Model.ConsigneeList, "ConsignorId", "PartyNameWithCode"), "Select", new { @style = "width:230px" })
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Invoice No</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.ConsigneeInvoiceNo, new { @class = "width115" })
                                            </td>
                                            <td>Service Tax Regn.No</td>
                                            <td>@Html.TextBoxFor(model => model.ConsigneeServiceTaxRegNo, new { @class = "width115" })</td>
                                        </tr>
                                        <tr>
                                            <td colspan="4">Consignor&#39;s Name &amp; Address with Phone NO.</td>
                                        </tr>
                                        <tr>
                                            <td colspan="4">
                                                @Html.DropDownListFor(m => m.ConsignorId, new SelectList(Model.ConsignorList, "ConsignorId", "PartyNameWithCode"), "Select", new { @style = "width:230px" })
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Purchase Order No.</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.ConsignorPurchaseOrderNo, new { @class = "width115" })
                                            </td>
                                            <td>Service Tax Regn.No</td>
                                            <td>@Html.TextBoxFor(model => model.ConsignorServiceTaxRegNo, new { @class = "width115" })</td>
                                        </tr>
                                    </table>
                                </td>
                                <td style="vertical-align: top; width: 615px; float: left">
                                    <table cellspacing="0" cellpadding="0">
                                        <tr>
                                            <td style="width: 80px">PAN NO.</td>
                                            <td style="width: 210px">
                                                @Html.TextBoxFor(model => model.PanNo, new { @class = "width115" })</td>
                                            <td colspan="2" style="width: 320px">Centralised Service Tax Regn. No. :
                                                @Html.TextBoxFor(model => model.CentralisedServiceTaxRegnNo, new { @class = "width100" })</td>
                                        </tr>
                                        <tr>
                                            <td colspan="2">From CODE & Name</td>
                                            <td colspan="2">CONSIGNMENT NOTE NO.</td>
                                        </tr>
                                        <tr>
                                            <td colspan="2" rowspan="4">
                                                @Html.TextAreaFor(model => model.FromCode, new { @class = "width250", @rows = "3" })</td>
                                            <td colspan="2">
                                                @Html.TextBoxFor(model => model.ConsignmentNoteNo, new { @class = "width250" })</td>
                                        </tr>
                                        <tr>
                                            <td>Date : </td>
                                            <td>
                                                @(Html.Telerik().DatePickerFor(m => m.ConsignmentDate).InputHtmlAttributes(new { @class = "input-block-level" }))
                                            </td>
                                        </tr>
                                        <tr>
                                            <td colspan="2">Address of Issuing Office</td>
                                        </tr>
                                        <tr>
                                            <td colspan="2" rowspan="2">
                                                @Html.TextAreaFor(model => model.AddressofIssuing, new { @class = "width250", @rows = "2" })</td>
                                        </tr>
                                        <tr>
                                            <td colspan="2">To CODE &amp; Name</td>
                                        </tr>
                                        <tr>
                                            <td colspan="2" rowspan="2">
                                                @Html.TextAreaFor(model => model.ToCode, new { @class = "width250", @rows = "3" })</td>
                                            <td colspan="2">Phone No</td>
                                        </tr>
                                        <tr>
                                            <td colspan="2">
                                                @Html.TextAreaFor(model => model.CompanyPhone, new { @class = "width250", @rows = "2" })</td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    <table cellspacing="0" cellpadding="0">
                                        <tr>
                                            <td style="width: 120px">No.of Package</td>
                                            <td style="width: 220px">Method of Packing</td>
                                            <td style="width: 135px">Actual Wt.In Kgs.</td>
                                            <td style="width: 135px">Charged Wt. in Kgs./<br />
                                                Category of Load</td>
                                            <td style="width: 170px">Rate</td>
                                            <td style="width: 230px">Charges</td>
                                            <td style="width: 230px">Amount (Rs.)</td>
                                        </tr>
                                        <tr>
                                            <td>
                                                @Html.TextBoxFor(model => model.NoofPackages, new { @class = "width100" })</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.MethodOfPacking, new { @class = "width200" })</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.ActualWtInKGS, new { @class = "width115" })</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.CategoryOfLoad, new { @class = "width115" })</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.Rate, new { @class = "width115" })</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.Charges1, new { @class = "width115" })</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.Amount1, new { @class = "width115", @onkeydown = "OnAmountValueChanged(event)" })</td>
                                        </tr>
                                        <tr>
                                            <td>LOAD TYPE</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.LoadType, new { @class = "width115" })
                                            </td>
                                            <td colspan="3">
                                                <table>
                                                    <tr>
                                                        <td style="width: 70px">Distance</td>
                                                        <td style="width: 125px">
                                                            @Html.TextBoxFor(model => model.Distance, new { @class = "width100" })</td>
                                                        <td style="width: 63px">Kms.</td>
                                                        <td>
                                                            @Html.TextBoxFor(model => model.KMS, new { @class = "width115" })</td>
                                                    </tr>
                                                </table>
                                            </td>
                                            <td>
                                                @Html.TextBoxFor(model => model.Charges2, new { @class = "width115" })</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.Amount2, new { @class = "width115", @onkeydown = "OnAmountValueChanged(event)" })</td>
                                        </tr>
                                        <tr>
                                            <td colspan="3">Description (said To Contain)</td>
                                            <td colspan="2">For paid Consignment/Advance Payment - Specify</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.Charges3, new { @class = "width115" })</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.Amount3, new { @class = "width115", @onkeydown = "OnAmountValueChanged(event)" })</td>
                                        </tr>
                                        <tr>
                                            <td colspan="3">
                                                @Html.TextBoxFor(model => model.DescriptionSaidToContain, new { @class = "width350" })</td>
                                            <td>MR.No.:</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.AdvancedPaymentMRNo, new { @class = "width115" })</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.Charges4, new { @class = "width115" })</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.Amount4, new { @class = "width115", @onkeydown = "OnAmountValueChanged(event)" })</td>
                                        </tr>
                                        <tr>
                                            <td colspan="3">Private Marks/Other Identification</td>
                                            <td>Date:</td>
                                            <td>
                                                @(Html.Telerik().DatePickerFor(m => m.AdvancedPaymentDate).InputHtmlAttributes(new { @class = "input-block-level" }))
                                            </td>
                                            <td>
                                                @Html.TextBoxFor(model => model.Charges5, new { @class = "width115" })</td>
                                            <td>
                                                @Html.TextBoxFor(model => model.Amount5, new { @class = "width115", @onkeydown = "OnAmountValueChanged(event)" })</td>
                                        </tr>
                                        <tr>
                                            <td colspan="3">
                                                @Html.TextBoxFor(model => model.PrivateMarketOtherIdentificaion, new { @class = "width350" })</td>
                                            <td>Amount :</td>
                                            <td>@Html.TextBoxFor(model => model.AdvancedPaymentAmount, new { @class = "width115" })</td>
                                            <td>Final Amount :</td>
                                            <td><span id="FinalAmount">0</span></td>
                                        </tr>
                                        <tr>
                                            <td>Business Type :</td>
                                            <td colspan="2">
                                                @Html.TextBoxFor(model => model.BusinessType, new { @class = "width250" })</td>
                                        </tr>
                                        <tr>
                                            <td colspan="4">Dimension of Cinsignment (If Bulky / ODC)</td>
                                            <td>Sales Tax Form/Permit/Declaration No.</td>
                                        </tr>
                                        <tr>
                                            <td colspan="5">
                                                <table cellspacing="0" cellpadding="0">
                                                    <tr>
                                                        <td style="width: 90px">Length</td>
                                                        <td style="width: 90px">Width</td>
                                                        <td style="width: 90px">Height</td>
                                                        <td style="width: 90px">No.of 
                                                                <br />
                                                            Pkgs.</td>
                                                        <td style="width: 90px">Total CFT
                                                                <br />
                                                            CMT</td>
                                                        <td style="width: 100px">Single
                                                             <br />
                                                            piece weight</td>
                                                        <td style="width: 100px">
                                                            @Html.TextBoxFor(model => model.DeclarationNo, new { @class = "width100" })</td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            @Html.TextBoxFor(model => model.DimensionLength, new { @class = "width70" })
                                                        </td>
                                                        <td>
                                                            @Html.TextBoxFor(model => model.DimensionWidth, new { @class = "width70" })</td>
                                                        <td>
                                                            @Html.TextBoxFor(model => model.DimensionHeight, new { @class = "width70" })</td>
                                                        <td>
                                                            @Html.TextBoxFor(model => model.DimensionNoOfPKGS, new { @class = "width70" })</td>
                                                        <td>
                                                            @Html.TextBoxFor(model => model.DimensionTotalCFTCMT, new { @class = "width70" })</td>
                                                        <td>
                                                            @Html.TextBoxFor(model => model.DimensionSinglePieceWeight, new { @class = "width70" })</td>
                                                        <td>Valid Type</td>
                                                    </tr>
                                                    <tr>
                                                        <td>&nbsp;</td>
                                                        <td>&nbsp;</td>
                                                        <td>&nbsp;</td>
                                                        <td>&nbsp;</td>
                                                        <td>&nbsp;</td>
                                                        <td>&nbsp;</td>
                                                        <td>
                                                            @Html.TextBoxFor(model => model.ValidType, new { @class = "width115" })</td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td colspan="5">
                                                <table>
                                                    <tr>
                                                        <td style="width: 350px">Declared value of goods :</td>
                                                        <td style="width: 350px">
                                                            @Html.TextBoxFor(model => model.DeclaredValueOfGoods, new { @class = "width250" })</td>
                                                    </tr>
                                                    <tr>
                                                        <td style="width: 350px">Service Tax Through :</td>
                                                        <td style="width: 350px">
                                                            @Html.TextBoxFor(model => model.ServicetaxThrough, new { @class = "width250" })</td>
                                                    </tr>
                                                    <tr>
                                                        <td style="width: 350px">Basic of Booking</td>
                                                        <td style="width: 350px">
                                                            @Html.TextBoxFor(model => model.BasicOfBooking, new { @class = "width250" })</td>
                                                    </tr>
                                                    <tr>
                                                        <td style="width: 350px">Billed with M/s</td>
                                                        <td style="width: 350px">
                                                            @Html.TextBoxFor(model => model.BilledWithMs, new { @class = "width250" })</td>
                                                    </tr>
                                                    <tr>
                                                        <td style="width: 350px">Branch Code :</td>
                                                        <td style="width: 350px">
                                                            @Html.TextBoxFor(model => model.BranchCode, new { @class = "width250" })</td>
                                                    </tr>
                                                    <tr>
                                                        <td style="width: 350px">Party Code :</td>
                                                        <td style="width: 350px">
                                                            @Html.TextBoxFor(model => model.PartyCode, new { @class = "width250" })</td>
                                                    </tr>
                                                    <tr>
                                                        <td style="width: 350px">To Pay/Paid/TBB - Amount (in words)</td>
                                                        <td style="width: 350px">
                                                            @Html.TextBoxFor(model => model.ToPay, new { @class = "width250" })</td>
                                                    </tr>
                                                    <tr>
                                                        <td style="width: 350px">Rebooking / Included</td>
                                                        <td style="width: 350px">
                                                            @Html.TextBoxFor(model => model.Rebooking, new { @class = "width250" })</td>
                                                    </tr>
                                                    <tr>
                                                        <td style="width: 350px">Origional /Main Consignment No. with Stn. Code</td>
                                                        <td style="width: 350px">
                                                            @Html.TextBoxFor(model => model.MainConsignmentNo, new { @class = "width250" })</td>
                                                    </tr>
                                                    <tr>
                                                        <td style="width: 350px">Service Taxis Payable By</td>
                                                        <td style="width: 350px">
                                                            @Html.DropDownListFor(m => m.ServiceTaxisPayableBy, Model.ServiceTaxisPayableByList, "Select")</td>
                                                    </tr>

                                                </table>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>&nbsp;</td>
                                            <td>&nbsp;</td>
                                            <td>&nbsp;</td>
                                            <td>&nbsp;</td>
                                            <td>&nbsp;</td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td colspan="3">
                        <div class="row-fluid">
                            <div class="span12">
                                <label class="PageTitleHeading"></label>
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <button type="submit" class="btn btn-success" @*onclick="OnConsignmentNoteSave()"*@>Save</button>
                        <button type="button" class="btn btn-info" onclick="OnSaveCancelButton('ConsignmentNote','Index')">Cancel</button>
                    </td>
                    <td></td>
                    <td></td>
                </tr>
            </table>
        </div>
    </section>
}
<script src="~/Content/Scripts/ConsignmentNote.js"></script>
